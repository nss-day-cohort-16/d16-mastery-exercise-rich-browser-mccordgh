{{#each toys}}
	
	<div id="card--{{@index}}" class="toyCard col-md-2">
		
		<div class="cardHeader">
			<button type="button" class="delButton pull-right" data--id="{{id}}">X</button>
			<h3>{{name}}</h3>
		</div>
		
		<div class="cardImageDiv">
		 <img src="{{url}}" class="cardImage" id="{{id}}">
		</div>
		
		<div class="cardDesc">
			<p class="price">${{price}}</p>
			<p class="desc">{{desc}}</p>
		</div>
	
	</div>

{{/each}}